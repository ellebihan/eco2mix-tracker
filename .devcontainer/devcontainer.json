{
  "name": "airflow-dbt-dev",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "extensions": [
    "ms-python.python"
  ],
  "settings": {
    "python.pythonPath": "/usr/local/bin/python"
  },
  "mounts": [
    "source=${localWorkspaceFolder}/dags,target=/opt/airflow/dags,type=bind",
    "source=${localWorkspaceFolder}/dbt_elsa,target=/opt/airflow/dbt_elsa,type=bind",
    "source=${localWorkspaceFolder}/scripts,target=/opt/airflow/scripts,type=bind",
    "source=${localWorkspaceFolder}/dbt_elsa/profiles.yml,target=/home/airflow/.dbt/profiles.yml,type=bind"
  ],
  "remoteUser": "airflow"
}
